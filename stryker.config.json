{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "packageManager": "npm",
  "reporters": [
    "html",
    "clear-text",
    "progress"
  ],
  "testRunner": "vitest",
  "coverageAnalysis": "perTest",
  "mutate": [
    "core/irontrust/**/*.ts",
    "!core/irontrust/**/*.test.ts"
  ],
  "thresholds": {
    "high": 100,
    "low": 99,
    "break": 100
  }
}
